{
  "name": "react-ssr",
  "version": "1.0.0",
  "description": "Server side rendering project",
  "main": "index.js",
  "scripts": {
    "start": "node build/server.bundle.js",
    "build": "npm-run-all test bundle:*",
    "bundle:client": "webpack --config webpack/webpack.client.js",
    "bundle:server": "webpack --config webpack/webpack.server.js",
    "dev": "npm-run-all --parallel lint:watch bundle:* server:watch",
    "server:watch": "nodemon --watch build --exec \"node build/server.bundle.js\"",
    "lint": "eslint ./src/app/",
    "lint:watch": "esw -w ./src/app/",
    "lint:fix": "eslint ./src/app/ --fix",
    "flow": "flow",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "setupFiles": [
      "./src/app/tests/setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "author": "Stefan Lazarevic",
  "license": "ISC",
  "dependencies": {
    "babel-jest": "^23.6.0",
    "classnames": "2.2.6",
    "jest-styled-components": "^6.2.1",
    "ls-react-grid": "0.2.0",
    "prop-types": "15.6.2",
    "react": "16.5.2",
    "react-dom": "16.5.2",
    "react-ga": "2.5.3",
    "react-helmet": "5.2.0",
    "react-loadable": "5.5.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "regenerator-runtime": "^0.12.1",
    "styled-components": "^3.4.9"
  },
  "devDependencies": {
    "autoprefixer": "9.1.5",
    "babel-core": "^6.26.0",
    "babel-eslint": "10.0.0",
    "babel-loader": "7.1.2",
    "babel-plugin-styled-components": "1.8.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-flow": "6.23.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "body-parser": "1.18.3",
    "clean-webpack-plugin": "0.1.19",
    "compression": "1.7.3",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.0",
    "cssnano": "4.1.0",
    "dotenv": "6.0.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "5.6.0",
    "eslint-config-prettier": "3.1.0",
    "eslint-plugin-flowtype": "2.50.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-react": "7.11.1",
    "eslint-watch": "4.0.2",
    "express": "4.16.3",
    "extract-text-webpack-plugin": "3.0.2",
    "flow-bin": "0.81.0",
    "html-webpack-inline-source-plugin": "0.0.10",
    "html-webpack-plugin": "2.30.1",
    "inline-manifest-webpack-plugin": "4.0.2",
    "isomorphic-fetch": "2.2.1",
    "isomorphic-style-loader": "4.0.0",
    "jest": "^23.6.0",
    "minify-css-string": "1.0.0",
